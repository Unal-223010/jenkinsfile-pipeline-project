pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Compiling the java source code'
                sh 'javac Hello.java'
            }
        }
        stage('calis run ') {
            steps {
                echo 'Running the compiled java code.'
                sh 'java Hello'
             }
         }
         stage('oyle') {
            steps {
                echo 'Bu is bu kadar'
                
            }
        }
    }
}
